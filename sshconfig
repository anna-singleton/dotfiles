AddKeysToAgent yes

host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github

host ellie
    HostName anna-banana.xyz
    User anna
    IdentityFile ~/.ssh/id_personal
    Port 8000

host unison
    HostName anna-banana.xyz
    User unison
    IdentityFile ~/.ssh/unison_key
    Port 8000

host *
    TCPKeepAlive yes
    ServerAliveInterval 120
    ControlPath /tmp/cm-%r@%h:%p
    ControlMaster auto
    ControlPersist 10m
